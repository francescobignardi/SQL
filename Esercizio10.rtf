SELECT CUSTOMER.NAME\
FROM CUSTOMER\
INNER JOIN ORDER1\
ON CUSTOMER.CUSTOMER_ID = ORDER1.CUSTOMER_ID;\
\
SELECT PRODUCT.NAME\
FROM PRODUCT\
INNER JOIN INVENTORY\
ON PRODUCT.PRODUCT_ID = INVENTORY.PRODUCT_ID;}