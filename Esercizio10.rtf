SELECT MEAL.NAME\
FROM MEAL\
INNER JOIN INGREDIENT\
ON MEAL.MEAL_ID = INGREDIENT.MEAL_ID\
WHERE INGREDIENT.NAME = 'Garlic';\
\
\
SELECT COUNT(DISTINCT MEAL.NAME)\
FROM MEAL \
INNER JOIN INGREDIENT \
ON MEAL.MEAL_ID = INGREDIENT.MEAL_ID\
WHERE INGREDIENT.VEGETARIAN = TRUE;\
\
ALTER TABLE INGREDIENT \
ADD FOREIGN KEY(MEAL_ID) REFERENCES MEAL(MEAL_ID);\
\
\
SELECT m.NAME \
FROM MEAL m\
INNER JOIN INGREDIENT i \
ON m.MEAL_ID = i.MEAL_ID \
WHERE i.GLUTEN_FREE AND i.VEGAN\
GROUP BY m.NAME;\
\
SELECT MEAL_NAME, PRICE, (SELECT AVG(PRICE) FROM meal_order_with_details mowd2) AS media\
from meal_order_with_details mowd\
GROUP BY MEAL_NAME , PRICE\
HAVING media > PRICE;\
\
\
SELECT m.NAME\
FROM MEAL m\
INNER JOIN INGREDIENT i \
ON m.MEAL_ID = i.MEAL_ID \
GROUP BY m.NAME \
HAVING COUNT(i.NAME)>5;\
\
